<% if(success && success.length) {%>
    <div class="alert alert-dismissible fade show" role="alert" id="success" 
         style="text-align: center; color: greenyellow;"> 
        <h1><%= success %></h1>
    </div>
<% } %> 

<% if(error && error.length) {%>
    <div class="alert alert-dismissible fade show" role="alert" id="error">
        <%= error %>
    </div>
<% } %> 

